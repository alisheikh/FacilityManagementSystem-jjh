<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="Main.Entities.Facility.UnitImpl">
    <class name="UnitImpl" table="unit">
        <id name="id" column="id"/>
        <many-to-one name="facilityId" column="facility_id"/>
        <property name="capacity" column="capacity"/>
        <property name="unitNumber"  column="unit_Number"/>
    </class>
</hibernate-mapping>