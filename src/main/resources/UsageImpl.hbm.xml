<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="Main.Entities.usage">
    <class name="UnitUsageImpl" table="unit_usage">
        <id name="id" column="id"/>
        <property name="startTime" column="start_time"/>
        <property name="endTime"  column="end_time"/>
        <one-to-one name="unit" class="Main.Entities.Facility.Unit" cascade="save-update, delete"/>
        <one-to-one name="unitUser" class="Main.Entities.usage.UnitUser" cascade="save-update, delete"/>
    </class>
</hibernate-mapping>