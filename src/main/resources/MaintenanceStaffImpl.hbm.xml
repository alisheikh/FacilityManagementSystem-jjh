<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="Main.Entities.maintenance">
    <class name="MaintenanceStaffImpl" table="unit_user">
        <id name="id" column="id" type="java.lang.Integer">
            <generator class="increment" />
        </id>
        <property name="firstName" type = "string" column="first_name"/>
        <property name="lastName" type="string" column="last_name"/>
        <property name="phoneNumber" type ="java.lang.Integer" column="phone_number"/>
        <property name="emailAddress" type="string" column="email_address"/>
    </class>
</hibernate-mapping>