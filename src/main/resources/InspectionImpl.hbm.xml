<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="Main.Entities.maintenance">
    <class name="InspectionImpl" table="inspection">
        <id name="id" column="id" type="java.lang.Integer">
            <generator class="increment" />
        </id>
        <property name="inspectionDate">
            <column name="inspection_date" sql-type="timestamp" length="29" precision="6"/>
        </property>

        <many-to-one name="facility" class="Main.Entities.Facility.FacilityImpl" cascade="save-update, delete"/>
        <many-to-one name="maintenanceStaff" class="Main.Entities.maintenance.MaintenanceStaffImpl" cascade="save-update, delete"/>
    </class>
</hibernate-mapping>